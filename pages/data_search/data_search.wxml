<!--pages/data_search/data_search.wxml-->
 <view class="cu-bar search bg-white">
    <view class='search-form round'>
      <text class="icon-search"></text>
      <input type="text" placeholder="食物信息查询" confirm-type="search"></input>
    </view>
    <view class='action'>
      <button class='cu-btn bg-blue shadow-blur round'>食物信息查询</button>
    </view>
  </view>
<view class="cu-list grid col-4">
  <view class="cu-item" wx:for="{{content}}">
    <text>{{item.value}}</text>
  </view>
</view>
<tabbar active="/pages/search/search"></tabbar>
