      <image src='/images/baike1.png' data-page="/pages/proposal/proposal"></image>

    </view>
    <view data-page="/pages/proposal/proposal" class='{{active == "/pages/proposal/proposal" ? "text-blue" : "text-gray"}}'>饮食建议</view>
  </view>
   <view class="action" bindtap='navigator' data-page="/pages/index/index">
    <view class='icon-cu-image' data-page="/pages/index/index">
      <image src='/images/geren1.png' data-page="/pages/index/index"></image>

    </view>
    <view data-page="/pages/index/index" class='{{active == "/pages/index/index" ? "text-blue" : "text-gray"}}'>个人信息</view>
  </view>
</view>