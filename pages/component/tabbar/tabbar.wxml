      <image src='/images/home1.png' data-page="/pages/home/home"></image>
    </view>
    <view data-page="/pages/home/home" class='{{active == "/pages/home/home" ? "text-blue" : "text-gray"}}'>主页</view>
  </view>

  <view class="action" bindtap='navigator' data-page="/pages/search/search">
    <view class='icon-cu-image' data-page="/pages/search/search">
