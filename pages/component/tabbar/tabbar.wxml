    </view>
    <view data-page="/pages/search/search" class='{{active == "/pages/search/search" ? "text-blue" : "text-gray"}}'>食品查询</view>
  </view>

  <view class="action" bindtap='navigator' data-page="/pages/proposal/proposal">
